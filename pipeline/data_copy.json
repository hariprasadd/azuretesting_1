{
	"name": "data_copy",
	"properties": {
		"activities": [
			{
				"name": "copy_data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "FileServerReadSettings",
							"maxConcurrentConnections": 2,
							"recursive": true,
							"modifiedDatetimeStart": {
								"value": "if object_id('dbo.control_tbl') is null select formatDateTime('1999-01-01 00:00:00') else select max(last_end_ts) from dbo.control_tbl;",
								"type": "Expression"
							},
							"modifiedDatetimeEnd": {
								"value": "@utcnow()",
								"type": "Expression"
							},
							"wildcardFileName": "*",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "source_data",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}